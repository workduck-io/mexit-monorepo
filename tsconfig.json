{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "es2021",
      "dom"
    ],
    "noImplicitAny": false,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "inlineSourceMap": true,
    "paths": {
      "*": [
        "node_modules/*"
      ]
    },
  },
  "include": [
    "src"
  ]
}