{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "module": "esnext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "target": "es5",
    "noImplicitAny": false,
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
  "include": [
    "src"
  ]
}