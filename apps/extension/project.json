{
  "root": "apps/extension",
  "sourceRoot": "apps/extension/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/extension",
        "index": "apps/extension/src/index.html",
        "baseHref": "/",
        "main": "apps/extension/src/index.tsx",
        "polyfills": "apps/extension/src/polyfills.ts",
        "tsConfig": "apps/extension/tsconfig.json",
        "assets": [
          "apps/extension/src/Assets",
          "apps/extension/src/manifest.json"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/extension/webpack.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "dev": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "extension:build",
        "hmr": true,
        "port": 3000
      },
      "configurations": {
        "production": {
          "buildTarget": "extension:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/extension/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}