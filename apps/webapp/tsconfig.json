{
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "target": "ESNext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "es2021",
      "dom"
    ],
    "noImplicitAny": false,
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": [
        "./node_modules/*"
      ]
    },
  },
  "include": [
    "./src/**/*"
  ]
}