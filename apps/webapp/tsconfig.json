{
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "target": "ESNext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "es2021",
      "dom"
    ],
    "noImplicitAny": false,
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
  "include": [
    "./src/**/*"
  ],
  "extends": "../../tsconfig.base.json"
}