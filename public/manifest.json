{
  "name": "Mexit",
  "description": "Extension built with React + TS for Chrome",
  "version": "0.0.1",
  "manifest_version": 2,
  "commands": {
    "open-kbar": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      },
      "description": "Open kbar"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    "Assets/*"
  ],
  "content_security_policy": "script-src 'self' https://browser.sentry-cdn.com https://cdn.mxpnl.com https://cdn4.mxpnl.com; object-src 'self'"
}